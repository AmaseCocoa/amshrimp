{
  "compilerOptions": {
    "noEmitOnError": false,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noUnusedParameters": false,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true,
    "declaration": false,
    "sourceMap": false,
    "target": "es6",
    "module": "commonjs",
    "removeComments": false,
    "noLib": false
  },
  "compileOnSave": false,
  "include": [
    "./**/*.ts"
  ],
  "exclude": [
    "./web/app/**/*.ts"
  ]
}
